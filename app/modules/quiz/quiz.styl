.section--intro {
	@media landscapeTablet {}
}

.section--intro::after {
	position: absolute;
	z-index: 0;
	right: 0;
	bottom: -2px;

	display: block;
	width: 284px;
	height: 270px;


	content: '';

	background-image: url('/assets/images/paper.png');
	background-repeat: no-repeat;
	background-size: contain;

	@media mobile {
		width: 374px;
		height: 356px;
	}

	@media landscapeTablet {
		width: 434px;
		height: 414px;
	}
}

.section--intro::before {
	position: absolute;
	z-index: 0;
	top: -20px;
	right: 226px;


	display: block;

	width: 92px;
	height: 93px;

	content: '';

	background-image: url('/assets/images/head.png');
	background-repeat: no-repeat;
	background-size: contain;

	@media mobile {
		right: 376px;
		height: 113px;

	}

	@media landscapeTablet {
		top: 49px;
		right: 316px;
	}
}

.section--intro .section-container::after {
	position: absolute;
	z-index: 0;
	right: 272px;

	bottom: 139px;

	display: block;

	width: 108px;

	height: 118px;

	content: '';

	background-image: url('/assets/images/money.png');
	background-repeat: no-repeat;
	background-size: contain;

	@media mobile {
		right: 452px;
		width: 158px;

	}

	@media landscapeTablet {
		right: 482px;
		bottom: 129px;

		width: 197px;
		height: 147px;
	}
}

.section--intro .section-header::before {
	position: absolute;
	top: -40px;
	right: -24px;

	display: block;

	width: 65px;
	height: 46px;

	content: 'Тест';
	text-align: center;

	color: #FFFFFF;
	background-image: url("data:image/svg+xml,%3C%3Fxml version='1.0' encoding='UTF-8'%3F%3E%3Csvg width='66px' height='47px' viewBox='0 0 66 47' version='1.1' xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink'%3E%3Ctitle%3EGroup 4%3C/title%3E%3Cg id='Page-1' stroke='none' stroke-width='1' fill='none' fill-rule='evenodd'%3E%3Cg id='editorial_article_enter' transform='translate(-766.000000, -281.000000)'%3E%3Cg id='bg' transform='translate(273.000000, 211.500000)'%3E%3Cg transform='translate(0.000000, 0.320312)' id='Group-5'%3E%3Cg transform='translate(49.000000, 69.179688)'%3E%3Cg id='Group-4' transform='translate(444.125000, 0.369141)'%3E%3Cpolygon id='Rectangle' fill='%23BA4961' points='50 12 65 0 65 34 50 46'%3E%3C/polygon%3E%3Crect id='Rectangle' stroke='%23E25A76' fill='%23E25A76' x='0.5' y='0.5' width='64' height='33'%3E%3C/rect%3E%3C/g%3E%3C/g%3E%3C/g%3E%3C/g%3E%3C/g%3E%3C/g%3E%3C/svg%3E");

	font-size: 18px;
	line-height: 33px;
}

.section--progress,
.section--result {
	padding: 40px 28px 45px;

	@media landscapeTablet {
		padding: 25px 0 45px 0;

	}
}

.section--progress .section-container,
.section--result .section-container {
	justify-content: flex-start;

	max-width: 498px;
	min-height: 466px;
	margin: 0;

	@media landscapeTablet {
		margin: 0 auto;
	}
}

.section--result .section-container {
	justify-content: flex-start;

	max-width: 498px;
	min-height: 466px;
}

.section-button {
	z-index: 1;
}

.answers-list {
	display: flex;
	align-items: flex-start;
	flex-flow: column nowrap;

	margin: 30px 0;
}

.counter {
	padding-bottom: 16px;

	color: #000000;

	font-size: 16px;
	line-height: 22px;
}

.question {
	color: #000000;

	font-family: 'Roboto Medium';
	font-size: 26px;
	line-height: 36px;
}

.answers-list input {
	position: absolute;

	width: 0;
	height: 0;

	cursor: pointer;

	opacity: 0;
}

.answers-list label,
.user-answer {
	margin: 4px 0;
	padding: 13px 24px;

	color: #000000;
	border: 1px solid #EDCDD3;
	border: none;
	border-radius: 2px;
	background: #FFFFFF;
	box-shadow: inset 0px 0px 0px 1px rgba(237, 205, 211, 1);
	transition: box-shadow 150ms;

	font-size: 16px;
	line-height: normal;
}

.answers-list label:hover {
	cursor: pointer;

	@media landscapeTablet {
		box-shadow: inset 0px 0px 0px 2px rgba(67, 164, 87, 1);
	}

}

.user-answer.correct {
	color: #43A457;
	border-radius: 2px;
	background: #FFFFFF;
	box-shadow: inset 0px 0px 0px 2px rgba(67, 164, 87, 1);
}

.user-answer.incorrect {
	color: #E25A76;
	border-radius: 2px;
	background: #FFFFFF;
	box-shadow: inset 0px 0px 0px 2px rgba(226, 90, 118, 1);
}

.answer-desc {
	flex-grow: 2;

	margin: 16px 0;

	color: #000000;

	font-size: 16px;
	line-height: 23px;

	@media landscapeTablet {
		margin: 0;
	}
}

.answer-desc a {
	text-decoration: underline;

	color: #00A0F6;
}

.result-title {
	z-index: 1;

	max-width: 350px;
	margin: 10px 0 41px;

	color: #000000;

	font-family: 'Roboto Black';
	font-size: 36px;
	line-height: 42px;

	@media mobile {
		margin: 60px 0 41px;
	}


}

.result-image {
	position: absolute;
	z-index: 0;
	right: 0;
	bottom: 40px;

	display: block;

	width: 318px;
	height: 346px;

	background-repeat: no-repeat;
	background-size: cover;

	@media mobile {
		bottom: auto;

		width: 518px;
		height: 536px;
	}

}

.share-buttons {
	z-index: 1;

	flex-grow: 2;
}

.share-container {
	display: flex;
	flex-flow: row wrap;
}

.share {
	display: flex;
	display: flex;
	align-items: center;
	justify-content: center;

	min-width: 60px;
	height: 50px;
	margin: 5px;
	padding: 0 15px;

	letter-spacing: 0;

	border-radius: 2px;
	background: #FFFFFF;
	box-shadow: 0 15px 22px 0 rgba(121, 54, 68, 0.23);
	transition: box-shadow 500ms;

	font-family: 'Roboto Bold';
	font-size: 17px;
}

a:hover {
	box-shadow: 0 15px 22px 0 rgba(121, 54, 68, 0.1);
	transition: box-shadow 150ms;
	color: #4267B2;
}

.share--fb {
	padding: 0px 18px;

	color: #4267B2;
}

